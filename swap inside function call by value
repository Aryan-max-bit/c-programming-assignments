#include <stdio.h>

void swap(int x, int y) {
    int temp = x;
    x = y;
    y = temp;
    printf("Inside function after swapping: x = %d, y = %d\n", x, y);
}

void main() {
    int a = 5, b = 10;
    printf("Before function call: a = %d, b = %d\n", a, b);
    swap(a, b);
    printf("After function call: a = %d, b = %d\n", a, b);
 
}
