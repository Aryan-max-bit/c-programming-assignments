#include <stdio.h>

int power(int a, int b) {
    int result = 1;
    for(int i = 1; i <= b; i++) {
        result = result * a;
    }
    return result;
}

void main() {
    int a, b;
    printf("Enter follwoing values base and exponent: ");
    scanf("%d %d", &a, &b);
    printf("%d^%d = %d", a, b, power(a, b));
 
}
